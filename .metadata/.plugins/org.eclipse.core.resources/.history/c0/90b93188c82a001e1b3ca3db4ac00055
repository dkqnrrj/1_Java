package edu.kh.oop.practice;

import package edu.kh.oop.practice.Book;

public class BookService {
	
	private Book title = null; 
	private Book price = null; 
	private Book discountRate = null; 
	private Book author = null; 
	
	
	public String toString() {
		
		
		return + "도서명은" + title + "입니다.";
		return + "가격은" + price + "입니다.";
		return + "할인율은" + discountRate + "입니다.";
		return + "저자명은" + author + "입니다.";
		
		
		this.title = title;
		this.price = price;
		this.discountRate = discountRate;
		this.author = author;
		
		
		
		
		
		
		
	}
	
	
	
	
	
	
	
	public void Practice() {
		
		
		
		
		
		
		
		
	}
	
	


}
